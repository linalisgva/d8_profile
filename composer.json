{
  "name": "linalisgva/d8_profile",
  "type": "drupal-profile",
  "description": "Drupal 8 installation profile.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "harvesthq/chosen",
        "version": "1.8.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    }
  ],
  "require": {
    "drupal/ctools": "^3.0.0",
    "drupal/pathauto": "^1.0.0",
    "drupal/token": "^1.1.0",
    "drupal/imce": "^1.6.0",
    "drupal/webform": "^5.0.0",
    "drupal/entity_reference_revisions": "^1.4.0",
    "drupal/paragraphs": "^1.2.0",
    "drupal/admin_toolbar": "^1.23.0",
    "drupal/environment_indicator": "^3.3.0",
    "drupal/workflow": "^1.0.0",
    "drupal/metatag": "^1.4.0",
    "drupal/field_group": "^3.0.0",
    "drupal/inline_entity_form": "^1.0.0",
    "drupal/search_api_solr": "^1.2.0",
    "drupal/cdn": "^3.2.0",
    "drupal/minifyhtml": "^1.1.0",
    "drupal/group": "^1.0.0",
    "drupal/svg_image": "^1.8.0",
    "drupal/crop": "^2.0.0",
    "drupal/eu_cookie_compliance": "^1.0.0",
    "drupal/config_filter": "^1.1.0",
    "drupal/config_split": "^1.3.0",
    "drupal/twig_vardumper": "^1.1.0",
    "drupal/devel": "^1.2.0",
    "harvesthq/chosen": "^1.8",
    "drupal/chosen": "^2.5.0"
  },
  "extra": {
    "installer-paths": {
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    }
  }
}
